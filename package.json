{
  "name": "jsonrpc-client-websocket",
  "version": "1.0.1",
  "description": "A websocket client that implements the JSON RPC 2.0 protocol",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm test && npm run lint",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabriciobastian/jsonrpc-client-websocket.git"
  },
  "bugs": {
    "url": "https://github.com/fabriciobastian/jsonrpc-client-websocket/issues"
  },
  "homepage": "https://github.com/fabriciobastian/jsonrpc-client-websocket#readme",
  "keywords": [
    "websocket",
    "jsonrpc2.0",
    "client"
  ],
  "author": "Fabricio Bastian",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "jest": "^24.9.0",
    "jest-websocket-mock": "^1.5.0",
    "mock-socket": "^9.0.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {}
}
